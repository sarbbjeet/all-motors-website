// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model VehicleGallary{
   id String @id @default(uuid())
   image String
}

model UsedCar{
  id String @id @default(uuid())
  make String
  model String
  detail String?
  year Int
  color String
  engine_size String
  transmission String
  mileage Int
  doors Int @default(4)
  fuel String
  image String?
  price Int 
  fprice Int?
  body_style String
}
